@article{barbosa-UseAdvancedChemometric-2014,
  title = {The Use of Advanced Chemometric Techniques and Trace Element Levels for Controlling the Authenticity of Organic Coffee},
  author = {Barbosa, Rommel Melgaço and Batista, Bruno Lemos and Varrique, Renan M. and Coelho, Vinicius A. and Campiglia, Andrés D. and Barbosa, Fernando},
