@article{lee2013,
  title = {Analysis of {{N-nitrosamines}} and Other Nitro(so) Compounds in Water by High-Performance Liquid Chromatography with Post-Column {{UV}} Photolysis/{{Griess}} Reaction},
  author = {Lee, Minju and Lee, Yunho and Soltermann, Fabian and family=Gunten, given=Urs, prefix=von, useprefix=true},
  date = {2013-09-15},
  journaltitle = {Water Research},
  shortjournal = {Water Research},
