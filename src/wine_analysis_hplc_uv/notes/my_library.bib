@article{peng_2021,
  title = {{{HPLC-DAD}} Fingerprints Combined with Chemometric Techniques for the Authentication of Plucking Seasons of {{Laoshan}} Green Tea},
  author = {Peng, Tian-Qin and Yin, Xiao-Li and Gu, Hui-Wen and Sun, Weiqing and Ding, Baomiao and Hu, Xian-Chun and Ma, Li-An and Wei, Shu-Dong and Liu, Zhi and Ye, Shi-Yi},
