  volume = {38},
  number = {19},
  pages = {1753--1758},
  issn = {1082-6076},
  doi = {10.1080/10826076.2015.1105256},
  abstract = {Detector-based comprehensive screening analysis of complex samples of natural origin using High Performance Liquid Chromatography (HPLC) can be a complicated and time-consuming task. There are a number of ways multidetection characterization can be achieved; however, there are limitations associated with each technique. Active Flow Technology (AFT) in Parallel Segmented Flow (PSF) mode allows for multiplexed detection HPLC analysis within a single injection, whereas maintaining chromatographic performance and allowing the use of multiple destructive detectors to achieve a comprehensive yet efficient screening of a complex sample. In this study, a comprehensive characterization analysis of tobacco leaf extract was carried out through multiplexed detection using a PSF column for the detection of biomolecules by UV-Vis detection, DPPH center dot for reactive-oxygen species (ROS) detection, and mass spectrometry, the latter two detection methods being sample destructive.},
  langid = {english},
  keywords = {active flow technology,DERIVATIZATION,DPPH center dot,high performance liquid chromatography,multiplexed detection,natural products,NATURAL-PRODUCTS,OUTLET FLOW,parallel segmented flow,PERFORMANCE LIQUID-CHROMATOGRAPHY,SEGMENTED FLOW CHROMATOGRAPHY,SEPARATIONS,TECHNOLOGY}
}

@article{pravadali-cekic2016,
  title = {Curtain {{Flow Column}}: {{Optimization}} of {{Efficiency}} and {{Sensitivity}}},
  author = {Pravadali-Cekic, S and Kocic, D and Hua, S and Jones, A and Dennis, G and Shalliker, A},
  date = {2016-06},
  journaltitle = {JOVE-JOURNAL OF VISUALIZED EXPERIMENTS},
  number = {112},
  issn = {1940-087X},
  doi = {10.3791/53471},
  abstract = {Active Flow Technology (AFT) is a form of column technology that increases the separation performance of a HPLC column through the use of a specially purpose built multiport end-fitting(s). Curtain Flow (CF) columns belong to the AFT suite of columns, specifically the CF column is designed so that the sample is injected into the radial central region of the bed and a curtain flow of mobile phase surrounding the injection of solute prevents the radial dispersion of the sample to the wall. The column functions as an 'infinite diameter' column. The purpose of the design is to overcome the radial heterogeneity of the column bed, and at the same time maximize the sample load into the radial central region of the column bed, which serves to increase detection sensitivity. The protocol described herein outlines the system and CF column set up and the tuning process for an optimized infinite diameter 'virtual' column.},
  langid = {english},
  keywords = {Active Flow Technology,Chemistry,COEFFICIENTS,Column Technology,Curtain Flow,DISPERSION,High Performance Liquid Chromatography,Issue 112,PERFORMANCE LIQUID-CHROMATOGRAPHY,RADIAL HETEROGENEITY,SEGMENTED OUTLET FLOW,SEPARATIONS,TECHNOLOGY,THROUGH-PUT}
}

