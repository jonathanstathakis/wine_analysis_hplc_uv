@article{adnan-ReliableDiscriminationGreen-2020,
  title = {Reliable {{Discrimination}} of {{Green Coffee Beans Species}}: {{A Comparison}} of {{UV-Vis-Based Determination}} of {{Caffeine}} and {{Chlorogenic Acid}} with {{Non-Targeted Near-Infrared Spectroscopy}}},
  shorttitle = {Reliable {{Discrimination}} of {{Green Coffee Beans Species}}},
  author = {Adnan, Adnan and Naumann, Marcel and Mörlein, Daniel and Pawelzik, Elke},
  date = {2020-06},
  journaltitle = {Foods},
  volume = {9},
  number = {6},
  pages = {788},
  publisher = {Multidisciplinary Digital Publishing Institute},
  issn = {2304-8158},
  doi = {10.3390/foods9060788},
  url = {https://www.mdpi.com/2304-8158/9/6/788},
  urldate = {2022-05-24},
  abstract = {Species adulteration is a common problem in the coffee trade. Several attempts have been made to differentiate among species. However, finding an applicable methodology that would consider the various aspects of adulteration remains a challenge. This study investigated an ultraviolet–visible (UV-Vis) spectroscopy-based determination of caffeine and chlorogenic acid contents, as well as the applicability of non-targeted near-infrared (NIR) spectroscopy, to discriminate between green coffee beans of the Coffea arabica (Arabica) and Coffea canephora (Robusta) species from Java Island, Indonesia. The discrimination was conducted by measuring the caffeine and chlorogenic acid content in the beans using UV-Vis spectroscopy. The data related to both compounds was processed using linear discriminant analysis (LDA). Information about the diffuse reflectance (log 1/R) spectra of intact beans was determined by NIR spectroscopy and analyzed using multivariate analysis. UV-Vis spectroscopy attained an accuracy of 97\% in comparison to NIR spectroscopy’s accuracy by selected wavelengths of LDA (95\%). The study suggests that both methods are applicable to discriminate reliably among species.},
  issue = {6},
  langid = {english},
  keywords = {Arabica,caffeine,chlorogenic acid,coffee,food fraud,linear discriminant analysis,NIR,Robusta,spectroscopy,UV-vis},
  file = {/Users/jonathan/OneDrive/Zotero/zotfile/Adnan et al. - 2020 - Reliable Discrimination of Green Coffee Beans Spec.pdf;/Users/jonathan/Zotero/storage/4DXYPCEP/788.html}
}

