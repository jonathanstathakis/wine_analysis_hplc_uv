@article{smajlovic-QuantitativeAnalysisDetection-2019b,
  title = {Quantitative Analysis and Detection of Chaptalization and Watering down of Wine Using Isotope Ratio Mass Spectrometry},
  author = {Smajlovic, I. and Wang, D. and Túri, M. and Qiding, Z. and Futó, I. and Veres, M. and Sparks, K. L. and Sparks, J. P. and Jakšić, D. and Vuković, A. and Vujadinović, M.},
