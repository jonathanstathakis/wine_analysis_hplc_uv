@article{çelik_2010,
  title = {Determination of Antioxidants by a Novel On-Line {{HPLC-cupric}} Reducing Antioxidant Capacity ({{CUPRAC}}) Assay with Post-Column Detection},
  author = {Çelik, Saliha Esin and Özyürek, Mustafa and Güçlü, Kubilay and Apak, Reşat},
  date = {2010-07},
  journaltitle = {Analytica Chimica Acta},
  shortjournal = {Analytica Chimica Acta},
  volume = {674},
  number = {1},
  pages = {79--88},
  issn = {00032670},
  doi = {10.1016/j.aca.2010.06.013},
  url = {https://linkinghub.elsevier.com/retrieve/pii/S0003267010007518},
  urldate = {2021-08-10},
  abstract = {A novel on-line HPLC-cupric reducing antioxidant capacity (CUPRAC) method was developed for the selective determination of polyphenols (flavonoids, simple phenolic and hydroxycinnamic acids) in complex plant matrices. The method combines chromatographic separation, constituent analysis, and post-column identification of antioxidants in plant extracts. The separation of polyphenols was performed on a C18 column using gradient elution with two different mobile phase solutions, i.e., MeOH and 0.2\% o-phosphoric acid. The HPLC-separated antioxidant polyphenols in the extracts react with copper(II)neocuproine (Cu(II)-Nc) reagent in a post-column reaction coil to form a derivative. The reagent is reduced by antioxidants to the copper(I)-neocuproine (Cu(I)-Nc) chelate having maximum absorption at 450 nm. The negative peaks of antioxidant constituents were monitored by measuring the increase in absorbance due to Cu(I)-Nc. The detection limits of polyphenols at 450 nm (in the range of 0.17–3.46 ␮M) after post-column derivatization were comparable to those at 280 nm UV detection without derivatization. The developed method was successfully applied to the identification of antioxidant compounds in crude extracts of Camellia sinensis, Origanum marjorana and Mentha. The method is rapid, inexpensive, versatile, non-laborious, uses stable reagents, and enables the on-line qualitative and quantitative estimation of antioxidant constituents of complex plant samples.},
  langid = {english},
  file = {/Users/jonathan/OneDrive/Zotero/zotfile/Çelik et al. - 2010 - Determination of antioxidants by a novel on-line H.pdf}
}

