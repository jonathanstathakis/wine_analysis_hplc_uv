@article{davis1994,
  title = {Statistical {{Theory}} of {{Overlap}} for {{Variable Poisson Density}}: {{Relaxation}} of {{Constraint}} of {{Randomness}}},
  shorttitle = {Statistical {{Theory}} of {{Overlap}} for {{Variable Poisson Density}}},
  author = {Davis, Joe M.},
  date = {1994},
  journaltitle = {Analytical chemistry (Washington)},
  volume = {66},
  number = {5},
  pages = {735--746},
  publisher = {American Chemical Society},
  location = {Washington, DC},
  issn = {0003-2700},
  doi = {10.1021/ac00077a025},
  abstract = {The statistical model of overlap is modified to address one-dimensional separations containing single-component peaks (SCPs) with variable Poisson densities. The resulting theory describes overlap in unsaturated separations, for which the variation of SCP density throughout the separation is known. The expected number of peaks is expressed by an integral, whose value depends on the average saturation of the separation, the number of detectable components, and a frequently function proportional to the SCP density. The theory is verified by its application to computer-simulated chromatograms containing SCPs with linear, quadratic, sinusoidal, and exponential frequencies. Procedures are proposed to estimate the frequency function from the distribution of chromatographic maxima and to estimate the number of detectable components in chromatograms. These procedures are verified by their application to computer-simulated chromatograms and to an experimental chromatogram of a well-characterized mixture. Computer simulations show that these procedures are more robust than those based on a constant Poisson density. A mathematical proof is offered, which shows that at constant low average saturation the maximum number of peaks in a separation containing SCPs with variable Poisson densities is obtained, when the SCP density is constant. This proof may explain previous experimental observations.},
  langid = {english},
  keywords = {Analysis,Analytical chemistry,Chemistry,Chromatographic analysis,Chromatographic methods and physical methods associated with chromatography,Exact sciences and technology,informationTheory MD,Methods,Poisson distribution,Statistics}
}

