  keywords = {2-D-HPLC,AROMATIC-COMPOUNDS,Coffee,Comprehensive heart cutting,LIQUID-CHROMATOGRAPHY,METHYLENE SELECTIVITY,PEAK DETECTION,Phenyl type stationary phase}
}

@article{stevenson2013,
  title = {Improved {{2D-HPLC}} of Red Wine by Incorporating Pre-Process Signal-Smoothing Algorithms: {{Liquid Chromatography}}},
  shorttitle = {Improved {{2D-HPLC}} of Red Wine by Incorporating Pre-Process Signal-Smoothing Algorithms},
