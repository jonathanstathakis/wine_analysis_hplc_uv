  shorttitle = {Carbohydrate Content, Dietary Fibre and Melanoidins},
  author = {Lopes, Guido R. and Ferreira, Andreia S. and Pinto, Mariana and Passos, Cláudia P. and Coelho, Elisabete and Rodrigues, Carla and Figueira, Cláudia and Rocha, Sílvia M. and Nunes, Fernando M. and Coimbra, Manuel A.},
