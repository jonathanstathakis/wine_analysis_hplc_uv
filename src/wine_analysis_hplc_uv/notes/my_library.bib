  keywords = {hydrophobicity,interactions,orthonasal,perception,polyphenols,retronasal,saliva,volatile organic compounds,volatility,wine matrix},
  file = {/Users/jonathan/OneDrive/Zotero/zotfile/Pittari et al. - 2021 - Interactions between Polyphenols and Volatile Comp.pdf;/Users/jonathan/Zotero/storage/JZJD2Q9A/Pittari et al. - 2021 - Interactions between Polyphenols and Volatile Comp.pdf}
}

