  file = {/Users/jonathan/OneDrive/Zotero/zotfile/Guiochon et al. - 2008 - Implementations of two-dimensional liquid chromato.pdf;/Users/jonathan/Zotero/storage/3M6T7TKI/S0021967308001271.html;/Users/jonathan/Zotero/storage/ZJX4PVL4/S0021967308001271.html}
